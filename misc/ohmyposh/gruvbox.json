{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "❯ ",
    "foreground": "#B48EAD",
    "foreground_templates": ["{{ if gt .Code 0 }}#BF616A{{ end }}"]
  },
  "console_title_template": "{{if .Root}}(Admin){{end}} {{.PWD}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }} ",
          "foreground": "blue",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "{{ .Path }} ",
          "foreground": "red",
          "type": "path",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin"
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "yellow",
          "type": "executiontime",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❯ ",
          "foreground": "green",
          "type": "status",
          "style": "plain",
          "foreground_templates": ["{{ if gt .Code 0 }}red{{ end }}"]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
